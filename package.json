{
  "name": "pathe-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:UNRULYEON/pathe-api.git",
  "author": "Amar Kisoensingh <amar@kisoensingh.sh>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start:prod": "yarn prisma:generate && yarn prisma:deploy && yarn prisma:seed && node dist/src/server.js",
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "seed": "ts-node --transpile-only src/prisma/seed.ts",
    "prepare": "husky install",
    "prisma:deploy": "cd src && npx prisma migrate deploy && cd ..",
    "prisma:generate": "cd src && npx prisma generate && cd ..",
    "prisma:seed": "yarn seed"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "node-cron": "^3.0.0",
    "node-telegram-bot-api": "^0.56.0",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@types/node-cron": "^3.0.1",
    "@types/node-telegram-bot-api": "^0.56.2",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.6.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
